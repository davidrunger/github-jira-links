{
  "manifest_version": 2,
  "name": "Github Jira Links",
  "version": "0.0.1",
  "description": "Change Jira issues references into clickable link in Github.",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["link.js"]
    }
  ],
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["storage"],
  "browser_specific_settings": {
    "gecko": {
      "id": "{dbcf2397-060a-41c6-983f-fee5f54c7ecb}"
    }
  }
}
